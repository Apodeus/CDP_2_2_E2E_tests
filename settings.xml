<settings>
    <profiles>
        <profile>
            <id>sonar</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <sonar.host.url>https://sonarcloud.io</sonar.host.url>
                <sonar.projectName>2_2_cdp</sonar.projectName>
                <sonar.projectKey>cdp:cdp</sonar.projectKey>
                <sonar.projectVersion>1.0.0</sonar.projectVersion>
                <sonar.sources>src/app</sonar.sources>
                <sonar.sourceEncoding>UTF-8</sonar.sourceEncoding>
                <sonar.login>${env.SONAR_TOKEN}</sonar.login>
                <sonar.organization>apodeus-github</sonar.organization>
            </properties>
        </profile>
    </profiles>
</settings>
